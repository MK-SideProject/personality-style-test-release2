<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".bankingtest.bankingtest_1_Activity">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/bankingtest_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/bankingtest_1_an1"
        android:layout_width="289dp"
        android:layout_height="72dp"
        android:background="@drawable/bankingtest_shape"
        android:text="월급통장, 생활비통장, 적금통장, 비상금통장 이미 통장을 분리해서 사용하고 있는 스타일"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.443">

    </Button>

    <Button
        android:id="@+id/bankingtest_1_an2"
        android:layout_width="301dp"
        android:layout_height="73dp"
        android:background="@drawable/bankingtest_shape"
        android:text="급여 통장에서 적금이나 필요한곳에 넣어두는거 말고는 분리는 못하지만 적당히 사용중인 스타일"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bankingtest_1_an1"
        app:layout_constraintVertical_bias="0.108" />

    <Button
        android:id="@+id/bankingtest_1_an3"
        android:layout_width="283dp"
        android:layout_height="51dp"
        android:background="@drawable/bankingtest_shape"
        android:text="딱히 관리하지않고 그냥 살아가는 스타일"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bankingtest_1_an2"
        app:layout_constraintVertical_bias="0.215" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".colortest.ColorTest_3">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/colortest_3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/colortest_3_an1"
        android:layout_width="182dp"
        android:layout_height="59dp"
        android:background="@drawable/colortest_shape"
        android:text="따뜻하고 포근한 느낌의 집"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView18"
        app:layout_constraintVertical_bias="0.546" />

    <Button
        android:id="@+id/colortest_3_an2"
        android:layout_width="185dp"
        android:layout_height="61dp"
        android:background="@drawable/colortest_shape"
        android:text="깔끔하고 심플한 대리석 집"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_3_an1"
        app:layout_constraintVertical_bias="0.111" />

    <Button
        android:id="@+id/colortest_3_an3"
        android:layout_width="177dp"
        android:layout_height="56dp"
        android:background="@drawable/colortest_shape"
        android:text="활동적인 가구가 많은 집"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_3_an2"
        app:layout_constraintVertical_bias="0.22" />

    <Button
        android:id="@+id/colortest_3_an4"
        android:layout_width="173dp"
        android:layout_height="58dp"
        android:background="@drawable/colortest_shape"
        android:text="마법도구와 책이 많은 집"
        app:layout_constraintBottom_toBottomOf="@+id/imageView18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_3_an3"
        app:layout_constraintVertical_bias="0.384" />


</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".triptest.triptest_result2_Activity">

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/triptest_result2">

    </ImageView>

    <Button
        android:id="@+id/triptest_restart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="다시하기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView43"
        app:layout_constraintHorizontal_bias="0.253"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9"
        android:background="@drawable/clearshape"/>

    <Button
        android:id="@+id/triptest_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="목록"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toEndOf="@+id/triptest_restart"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9"
        android:background="@drawable/clearshape" />

</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".colortest.colortest_1_Activity">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/colortest_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="48dp" />

    <Button
        android:id="@+id/colortest_1_an1"
        android:layout_width="101dp"
        android:layout_height="53dp"
        android:background="@drawable/colortest_shape"
        android:text="마을 촌장"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView16"
        app:layout_constraintVertical_bias="0.562" />

    <Button
        android:id="@+id/colortest_1_an2"
        android:layout_width="97dp"
        android:layout_height="49dp"
        android:background="@drawable/colortest_shape"
        android:text="어린 아이"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_1_an1"
        app:layout_constraintVertical_bias="0.135" />

    <Button
        android:id="@+id/colortest_1_an3"
        android:layout_width="101dp"
        android:layout_height="52dp"
        android:background="@drawable/colortest_shape"
        android:text="마을 주민"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_1_an2"
        app:layout_constraintVertical_bias="0.159" />

    <Button
        android:id="@+id/colortest_1_an4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/colortest_shape"
        android:text="로브를 두른 사람"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_1_an3"
        app:layout_constraintVertical_bias="0.372" />
</androidx.constraintlayout.widget.ConstraintLayout>
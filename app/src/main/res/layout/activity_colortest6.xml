<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".colortest.colortest_6_Activity">

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/colortest_6" />

    <Button
        android:id="@+id/colortest_6_an1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/colortest_shape"
        android:text="집에서 휴식을 즐긴다"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.535" />

    <Button
        android:id="@+id/colortest_6_an2"
        android:layout_width="130dp"
        android:layout_height="49dp"
        android:background="@drawable/colortest_shape"
        android:text="마을 구경을 한다"
        app:layout_constraintBottom_toBottomOf="@+id/imageView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_6_an1"
        app:layout_constraintVertical_bias="0.107" />

    <Button
        android:id="@+id/colortest_6_an3"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:background="@drawable/colortest_shape"
        android:text="도서관에 간다"
        app:layout_constraintBottom_toBottomOf="@+id/imageView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_6_an2"
        app:layout_constraintVertical_bias="0.151" />

    <Button
        android:id="@+id/colortest_6_an4"
        android:layout_width="132dp"
        android:layout_height="62dp"
        android:background="@drawable/colortest_shape"
        android:text="산책 나간다"
        app:layout_constraintBottom_toBottomOf="@+id/imageView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/colortest_6_an3"
        app:layout_constraintVertical_bias="0.268" />
</androidx.constraintlayout.widget.ConstraintLayout>